{
  "name": "jcc-ethereum-utils",
  "version": "0.1.2",
  "description": "Toolkit of crossing chain from Ethereum to SWTC chain",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "unpkg": "dist/jcc-ethereum-utils.min.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "deploy": "./publish.sh",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "gulp",
    "test:node": "tsc && nyc --reporter=html --reporter=text mocha --timeout=3000 test/*.spec.js",
    "test:browser": "tsc && karma start karma.conf.js",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "watch:tslint": "gulp dev",
    "compile": "./compile.sh"
  },
  "author": "JCCDex",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node": "^12.0.4",
    "bignumber.js": "^9.0.0",
    "ethereumjs-tx": "^2.0.0",
    "jcc_wallet": "^0.4.0",
    "web3": "1.0.0-beta.34"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-shell": "^0.7.0",
    "karma": "^4.0.1",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.1.4",
    "nyc": "^13.3.0",
    "sinon": "^7.3.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCCDex/jcc-ethereum-utils.git"
  },
  "keywords": [
    "cross-chain",
    "jccdex",
    "ethereum",
    "swtc-chain",
    "blockchain"
  ],
  "bugs": {
    "url": "https://github.com/JCCDex/jcc-ethereum-utils/issues"
  },
  "homepage": "https://github.com/JCCDex/jcc-ethereum-utils#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99
  },
  "files": [
    "lib",
    "dist"
  ]
}
